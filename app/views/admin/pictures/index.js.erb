var tinyMCEImageList = new Array(
        <% @pictures.each do |picture| %>
			["<%= picture.title %> (Large)", "<%= picture.photo_url(:large) %>"],						
			["<%= picture.title %> (Medium)", "<%= picture.photo_url(:medium) %>"],			
			["<%= picture.title %> (Small)", "<%= picture.photo_url(:small) %>"],						
			["<%= picture.title %> (Thumb)", "<%= picture.photo_url(:thumb) %>"]<% unless picture == @pictures.last %>,<% end %>									
        <% end %>
);